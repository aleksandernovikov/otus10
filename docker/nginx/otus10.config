server {
    listen 81 default_server;
    server_name _;
    charset utf-8;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        alias /code/static/;
    }

    location / {
        include uwsgi_params;
        proxy_pass http://unix:/run/gunicorn.sock;
    }
}